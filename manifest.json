{
    "manifest_version": 2,
    "name": "KlasPlugin",
    "description": "광운대학교 Klas 확장플러그인",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/logo/icon.png",
        "default_popup": "src/index.html"
    },
    "permissions": [
        "tabs",
        "https://*.kw.ac.kr/*",
        "http://*.kw.ac.kr/",
        "activeTab",
        "storage",
        "cookies"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self' "
}